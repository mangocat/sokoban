class Movecheck {
	function int movecheck(int x, int y, int x2, int y2){ 
		// (x, y) is where the player is, and (x2, y2) is the destination.
		var int idx; // the next 2 space's x location
		var int idy; // the next 2 space's y location
		let idx=(x2*2)-x;
		let idy=(y2*2)-y;
		if(peek(x2,y2)=0){
			return 1;
		};
		if(~(peek(x2,y2)=0) & peek(x2+3,y2+2)=0 & peek(idx,idy)=0){
			return 2;
		}
		return 0;
	}
}